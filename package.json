    "work": "run-p sass:watch live-server-work",
    "home": "run-p sass:watch live-server-home",
    "sass": "sass sass/style.sass css/style.css",
    "sass:watch": "sass --watch sass/style.sass:css/style.css",
    "browser-sync": "browser-sync start --server --files . --open external --host 192.168.0.36",
    "live-server-work": "live-server --host=192.168.0.36 --port=3000",
    "live-server-home": "live-server --host=192.168.1.123",
    "deploy": "npm run build && gh-pages -d dist"
  },
